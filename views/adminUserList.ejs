</head>
<body style="background-color: white;color: black;">
    <!-- navbar start -->
    
    <div id="stars"></div>
  <div class="container">
     <div class="container-fluide">
     <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
       
        <div class="container-fluid">
          
          <a class="navbar-brand" href="#"><img src="images/logo.png" alt="" id="logoimg"></a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav nav mx-auto mb-2 mb-lg-0">
              <li class="nav-item  a">
                <a class="nav-link" href="#">Home  | Admin Panel</a>
              </li>
            </ul>
          <div class="social">
          <ul class="navbar-nav mx-auto mb-2 mb-lg-0">
            <li class="nav-link ">
              <a href="/admin/adminUserList" style="color: black;">View User </i></a>
            </li>
            <li class="nav-link">
            <a href="/admin/orderManagement"  style="color: black;">view Order </a>
            </li>
            <li class="nav-link">
              <a href="/admin/adminLogout"  style="color: black;"> Logout</a>
            </li>
          </ul>
          <!-- <a href="Register.html" class="btn btn-danger  rounded-pill px-3 d-none d-lg-block">New Account +<i class="fa fa-arrow-right ms-3"></i></a> -->
        </div>
          </div>
      </nav> 
     </div>
   <!-- navbar end -->

  
<div class='container mt-5'> 
    <div  class='table-responsive'>
       <table class='table table-bordered table-striped'>
       <thead class='thead-dark bg-dark wow fadeIn text-white'>
       <tr>
       <th>S.no</th>
       <th>Username</th>
       <th>Email</th>
       <!-- <th>Password</th> -->
       <th>Mobile No.</th>
       <th>Address</th>
       <!-- <th>Diet</th> -->
       <!-- <th>Interests</th> -->
       <!-- <th>Status</th> -->
       <th>AdminVerify</th>
       <th>EmailVerify</th>
       <th>Update</th>
       <th>Delete</th>
       </tr>
       </thead>
       <tbody>
           <%userList.forEach((user,index)=>{%>
               <tr>
                    <td><%=index+1%></td>
                    <td><%=user.name%></td>
                    <td><%=user.email%></td>
                    <!-- <td><%=user.password%></td> -->
                    <td><%=user.contact%></td>
                    <td><%=user.address%></td>
                    <!-- <td><%=user.diet%></td> -->
                    <!-- <td><%=user.interests%></td> -->
                    <!-- <td><%=user.status%></td> -->
                    <td><a href="/admin/adminVerifyUser?userEmail=<%=user.email%>"><%=user.adminVerify%></a></td>
                    <td><a href=""><%=user.emailVerify%></a></td>
                     <td><a href="/admin/adminUserUpdate?userEmail=<%= user.email %>">Update</a></td>
                     <td><a href="/admin/adminUserDelete?userEmail=<%= user.email %>">Delete</a></td>
               </tr>
           <%})%>
         </tbody>
         </table>
      </div>
    </div>
  </div>
